package exercicios;

public class Teste {

	public static void main(String[] args) {
		System.out.println("Exercício um sendo executado:");
		
		Um operacao = new Um();
		Um mensagem = new Um();
		Um emprestimo = new Um();
		
		operacao.escolhaOperação(1, 2, 2);
		
		mensagem.printarMensagem(14);
		
		emprestimo.calcularEmprestimo(50.000, 24);
		
		System.out.println("------------------------------");
		
		System.out.println("Exercício dois sendo executado:");
		
		Dois ex2 = new Dois();
		ex2.calcularArea(5);
		ex2.toString();
		
		ex2.calcularArea(2, 6);
		ex2.calcularArea(5, 3, 4);
		
		System.out.println("------------------------------");
		
		System.out.println("Exercício três sendo executado:");
		
		Três ex3 = new Três();
		ex3.calcularArea(5);
		ex3.calcularArea(2, 6);
		ex3.calcularArea(5, 3, 4);
		
		System.out.println("------------------------------");

	}

